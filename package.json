{
  "name": "book_info_input",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.1.1",
    "cheerio": "^1.0.0-rc.2",
    "csv-writer": "^1.3.0",
    "lowdb": "^1.0.0",
    "node-fetch": "^2.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "1.1.5"
  },
  "devDependencies": {
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "foreman": "^3.0.1"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-scripts start",
    "pack": "electron-builder -w --ia32 --dir",
    "dist": "npm run build && build -w --ia32 --dir",
    "postinstall": "install-app-deps",
    "start": "nf start -p 3000"
  },
  "build": {
    "appId": "com.electron.bookInfoInput",
    "directories": {
      "buildResources": "public"
    },
    "extends": null
  }
